{
  "functions": {
    "api/opportunities.py": {
      "runtime": "python3.9"
    },
    "api/stats.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/opportunities",
      "destination": "/api/opportunities.py"
    },
    {
      "source": "/api/stats",
      "destination": "/api/stats.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}